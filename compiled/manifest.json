{
  "manifest_version": 2,
  "name": "Quick RDoc",
  "description": "This extension makes it easy to access Ruby Doc for classes",
  "version": "1.0",
  "background": {
    "persistent": false,
    "scripts": ["ruby-classes.js"]
  },
  "options_page": "options.html",
  "omnibox": { "keyword" : "rdoc" },
  "icons": {
    "16":  "16-color.png",
    "48":  "48-color.png",
    "128": "48-color.png"
  }
}
